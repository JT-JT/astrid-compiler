{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["./src", "./README.md", "./package.json"],
        "includePattern": ".+\\.js?$"
    },
    "plugins": ["plugins/summarize"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "recurse": true
    }
}